{
  "server": {
    "port": 3443,
    "host": "auto",
    "allowNetworkAccess": true
  },
  "ssl": {
    "keyPath": "./config/ssl/server.key",
    "certPath": "./config/ssl/server.crt"
  },
  "auth": {
    "sessionSecret": null,
    "sessionTimeout": 1800000,
    "maxLoginAttempts": 5
  },
  "claude": {
    "timeout": 30000,
    "maxConcurrent": 3,
    "rateLimit": {
      "requests": 100,
      "window": 60000
    }
  },
  "security": {
    "allowedDirectoriesFile": "./config/allow-dirs.txt",
    "passwordFile": "./config/passwd.txt"
  },
  "chatLog": {
    "enabled": true,
    "fileName": "chatlog.md"
  }
}